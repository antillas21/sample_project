<!DOCTYPE html>
<html>
<head>
  <title>SampleProject</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <ul>
      <% if current_user %>
        <%= link_to 'Logout', logout_path, method: :delete %>
      <% else %>
        <%= link_to 'Login', login_path %>
      <% end %>
    </ul>
  </nav>
  <div id="messages">
    <% if flash.any? %>
      <% flash.each do |k, v| %>
        <div class="flash <%= k %>"><%= v %></div>
      <% end %>
    <% end %>
  </div>

<%= yield %>

</body>
</html>
